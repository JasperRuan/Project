void EQ(){
int a;int b;int c;int d;int e;int f;
int g;int h;int i;int j;int k;int l;
int m;int n;int o;int p;int q;int r;
int s;int t;int u;int v;int w;int x;
int y;int z;
cout << "                                情商测试\n\n";
cout << "评定标准：同意请按1，不同意请按0, 输入答案后按下回车键以继续下一个问题\n\n";
cout << "一，与朋友发生争吵，你能在他面前掩饰你的沮丧\n";
cin >> a;
cout << "\n";
cout << "二，当工作进行不顺时，你认为这是对未来的一个警告\n";
cin >> b;
cout << "\n";
cout << "三，在您的朋友说话之前，你总能判断出他处于何种精神状态\n";
cin >>c;
cout << "\n";
cout << "四，当你担忧某件事时，夜里几小时都难以入睡\n";
cin >> d;
cout << "\n";
cout << "五，你认为大多数人必须经过更加的努力，不要轻言放弃\n";
cin >> e;
cout << "\n";
cout << "六，与朋友告诉你的好消息相比，你更容易受一部浪漫片的感染\n";
cin >> f;
cout << "\n";
cout << "七，当你的情况不妙时，你认为到了你该改变的时候了\n";
cin >> g;
cout << "\n";
cout << "八，经常想知道别人怎样看你\n";
cin >> h;
cout << "\n";
cout << "九，你对自己能使每个人高兴起来而高兴\n";
cin >> i;
cout << "\n";
cout << "十，你厌烦讨价还价，尽管它使你少花钱\n";
cin >> j;
cout << "\n";
cout << "十一，十分相信真率说话会使一切都变得容易\n";
cin >> k;
cout << "\n";
cout << "十二，尽管你知道自己是正确的也不愿来一场争吵\n";
cin >> l;
cout << "\n";
cout << "十三，做出一个决定会担心是否正确\n";
cin >> m;
cout << "\n";
cout << "十四，不担心环境的改变\n";
cin >> n;
cout << "\n";
cout << "十五，对于周末干什么，你总有很好的设想\n";
cin >> o;
cout << "\n";
cout << "十六，如果有一根魔棒，你会用它改变外貌和个性\n";
cin >> p;
cout << "\n";
cout << "十七，不管工作多么尽心尽力，你的上司似乎总在催促你\n";
cin >> q;
cout << "\n";
cout << "十八，你认为你的朋友或爱人对你寄予厚望\n";
cin >> r;
cout << "\n";
cout << "十九，你认为一点小压力不会伤害任何人\n";
cin >> s;
cout << "\n";
cout << "二十，你会把任何事都告诉最好的朋友，即便是个人隐私\n";
cin >> t;
cout << "\n\n";

u=(a+b+c+d+e+f+g+h+i+j+k+l+m+n+o+p+q+r+s+t);
if (u<=6){
    cout << "你必须多一点对别人的关心少关照自己，你喜欢打破社会常规，并且不担心通过疏远别人来取得自己想要的东西\n";
    cout << "你可能在短期内就会取得成果，但人们不久将会抱怨你，控制住你易冲动的天性，不要以粗暴的方式，而是试着迎合别人来得到你想要的一切\n";
    cout << "如果你得分不高不要沮丧，学会控制你的消极情感，充分利用积极情感\n";
}
if (6<u<=16){
        cout << "你意识到自己和他人的情感，但有时忽视他们，不明白这对你的幸福来说是多么重要，你对下一步的提升或买一幢房子这样的事关心支配着你的生活\n";
        cout << "然而，无论实现多少物质目的，你仍然感到不满足，试着去分析和理解你的感情，并按照它去行动，你会更幸福\n";
        cout << "记住，人们可能压抑你，使你暂时消沉，但是你总能在挫折中吸取教训，重新创造属于你的优势。\n";
}

if (16<=u){
    cout << "你对你的能力很是自信和放心，因此，当处于强烈的情感边缘时，你不会被击溃，即使在愤怒中，你也能有效的进行自我控制，保持彬彬有礼的君子风度\n";
    cout << "在控制你情绪这方面，你是出类拔萃的，与他人相处的很融洽\n";
    cout << "但是，太依赖社交技巧反而会忽视成功所需要的其他重要因素。\n";
}
cout << "\n\n谢谢您的耐心，您可以选择\n";
}


void SIXUE (){
int a;int b;
int c;int d;
int e;int f;
int g;int h;
int i;int j;
int k;int l;
int m;int n;
int o;int p;
int q;int r;
int s;int t;
int u;int v;
int w;int x;
int y;int z;
int A;int B;
int Q;
cout << "在朋友眼中，你属于挑剔一族？\n";
cout << "1.是的  2.不是\n";
cin >> a;
cout << "\n";
if (a==1){goto step2;}
if (a==2){goto step5;}
step2:
    cout << "遇上失败第一个念头就是落跑？\n";
    cout << "1.是的  2.不是\n";
    cin >> b;
    cout << "\n";
    if (b==1){goto step6;}
    if (b==2){goto step3;}
step3:
