    cout << "从古至今，相信没有人不曾身受过感情的困扰，然而究竟该如何从感情的牢笼中挣脱，让它也能成为你快乐的来源？\n";
    cout << "别无他法，唯有懂得喜欢自己。\n";
    cout << "因为通常感情出问题，只有清楚自己要什么，充满自信有安全感的女人，才能够充容不迫地解决\n";
    cout << "所以不妨静下心，重新检视问题关键，甚至可利用角色扮演，以第三人的角度去观察，将有助于了解事情本质，寻求理想的解决之道\n";
    cout << "另外，学习爱情保鲜计划，也是一个维系感情的方式，每天三次，一次三分钟，以肢体表达、言语沟通\n";
    cout << "让对方清楚知道你的关心，更是拥有长久爱情的好方式\n";
    cout << "\n";goto stepEND;
stepEEE:
    cout << "E型　人际关系（Friendship）\n";
    cout << "以和为贵是你信奉的人生哲学，希望大家能够和平相处\n";
    cout << "但若你在乎朋友的感受胜过于自己，那么就会出现被恶人吃定，不时攻城略地，逼得你哀怨，又只能咬牙忍耐，时日一久会使你罹患重度内伤。\n";
    cout << "\n解决方案：\n";
    cout << "很多时候，与朋友、同事、家人间互动关系得到的喜悦感，往往多过拥有百万财富\n";
    cout << "没有人可以独自生活，且因每个人都有自由意识，千万不要期待别人会如何改变，或是要掌控别人\n";
    cout << "就算你能够限制他的行为、时间，但也绝对无法改变他内心思想的运作\n";
    cout << "那么究竟该如何进入一个团体，或者是结交真正的好朋友呢？方法很简单，就是学会自我揭露、倾诉与关心\n";
    cout << "首先对周围朋友，产生极高的好奇心，然后将你所认识的人，作小小的等级分类，找出与你沟通较良好的对象，主动关心、倾诉，自然能提高和他人内心的共振\n";
    cout << "此外，练习自嘲的能力，别因过度在意别人的评价，而极度没有安全感，勇于承认自己的缺点，甚至偶尔可无伤地开开自己的小玩笑\n";
    cout << "久而久之，你会发现不一样的自己，在人群中还能更开心、有自信。\n";
    cout << "\n";goto stepEND;

stepEND:
    cout << "谢谢您的时间，您可以选择\n";
}


void HURT (){
    int a;
    int b;
    cout << "休息的假期，你到一个温泉里泡浴\n";
    cout << "当你正享受的时候\n一个身形和样子都惨不忍睹的人跳下去跟你一起泡\n";
    cout << "你的第一反应是什么？\n\n";
    cout << "1、不管他，继续泡 \n";
    cout << "2、心里生闷气，回自己房间\n";
    cout << "3、到其他的浴池再继续 　\n";
    cout << "4、脸上摆着不爽的表情，心里咒骂着\n";
    cin >> a;
    cout << "\n";

    cout << "选1、【不管他，继续泡】的朋友你最怕被揭穿“自己的口是心非“\n";
    cout << "这类型的人很希望保持完美的形象，很有礼貌有乖很客气，但心里的不喜欢不会说出口\n";
    cout << "这种最怕得罪人，非常压抑着自己的不满。 \n\n";
    cout << "选2、【心里生闷气，回自己房间】的朋友你最怕被揭穿“曾经犯下的错误”\n";
    cout << "这类型的人很善良，对于自己曾经犯下的错误，经常会自责，努力做到最好\n";
    cout << "若有天还是被揭穿，就等于在他们的伤口上撒盐。\n";
    cout << "\n选3、【到其他的浴池再继续】的朋友你最怕被揭穿“自己的随意生活习惯”\n";
    cout << "这类型的人很注重在外的形象，可是私底下生活却是很随性的很爱好自由\n";
    cout << "表面完美的她很怕被人家踢爆她原来卫生习惯这么差，她会觉得非常丢脸。\n";
    cout << "\n选4、【脸上摆着不爽的表情，心里咒骂着】的朋友你最怕被揭穿“自己的私生活不检点”\n";
    cout << "这类型的人表面上正气凛然\n";
    cout << "可是如果被人家发现她私底下生活也很狂野，不像表面上这么端庄贤淑的时候，她就会很生气，害怕自己的大好形象被毁了\n";
    cout << "\n\n谢谢您的耐心，您可以选择\n";
    cout << "\n";

}

void TAOTAI (){
int a;int b;
int c;int d;
int e;int f;
int g;int h;
int i;int j;
int k;int l;
int m;int n;
int o;int p;
int q;int r;
int s;int t;
int u;int v;
int w;int x;
int y;int z;

cout << "你不喜欢到陌生城市生活吗？\n";
cout << "1.是的  2.不是\n";
cin >> a;
cout << "\n";
if (a==1){goto step2;}
if (a==2){goto step3;}
step2:
    cout << "你总是会怕自己出错吗？\n";
    cout << "1.是的  2.不是\n";
    cin >> b;
    cout << "\n";
    if (b==1){goto step3;}
    if (b==2){goto step7;}
step3:
    cout << "你不是个不耻下问的人吗？\n";
    cout << "1.是的  2.不是\n";
    cin >> c;
    cout << "\n";
    if (c==1){goto stepCCC;}
    if (c==2){goto step6;}
step4:
    cout << "你对真理的态度是绝对拥护吗？\n";
    cout << "1.是的  2.不是\n";
    cin >> d;
    cout << "\n";
    if (d==1){goto step6;}
    if (d==2){goto step8;}
step5:
    cout << "你常分不清重点吗？\n";
    cout << "1.是的  2.不是\n";
    cin >> e;
    cout << "\n";
    if (e==1){goto step3;}
    if (e==2){goto step6;}
step6:
    cout << "你总觉得别人不会真心为自己好吗？\n";
    cout << "1.是的  2.不是\n";
    cin >> f;
    cout << "\n";
    if (f==1){goto step8;}
    if (f==2){goto step9;}
step7:
    cout << "你的思想算是比较保守的吗？\n";
    cout << "1.是的  2.不是\n";
    cin >> g;
    cout << "\n";
    if (g==1){goto step10;}
    if (g==2){goto step8;}
step8:
    cout << "你不懂得拒绝吗？\n";
    cout << "1.是的  2.不是\n";
    cin >> h;
    cout << "\n";
    if (h==1){goto step12;}
    if (h==2){goto step9;}
step9:
    cout << "你很不愿意自己吃亏吗？\n";
    cout << "1.是的  2.不是\n";
    cin >> i;
    cout << "\n";
    if (h==1){goto step10;}
    if (h==2){goto step15;}
step10:
    cout << "你总是固执己见吗？\n";
    cout << "1.是的  2.不是\n";
    cin >> j;
    cout << "\n";
    if (j==1){goto step16;}
    if (j==2){goto step11;}
step11:
    cout << "你觉得很多人都没品吗？\n";
    cout << "1.是的  2.不是\n";
    cin >> k;
    cout << "\n";
    if (k==1){goto step9;}
    if (k==2){goto step14;}
step12:
    cout << "你见不惯那种太高调的人吗？\n";
    cout << "1.是的  2.不是\n";
    cin >> l;
    cout << "\n";
    if (l==1){goto stepAAA;}
    if (l==2){goto step9;}
step13:
    cout << "你对很多事都表现出冷漠态度吗？\n";
    cout << "1.是的  2.不是\n";
    cin >> m;
    cout << "\n";
    if (m==1){goto step7;}
    if (m==2){goto step14;}
step14:
    cout << "你觉得大多数人说对的事就一定是对的吗？\n";
    cout << "1.是的  2.不是\n";
    cin >> n;
    cout << "\n";
    if (n==1){goto stepDDD;}
    if (n==2){goto step9;}
step15:
    cout << "你和别人说话总是要争个高低吗？\n";
    cout << "1.是的  2.不是\n";
    cin >> o;
    cout << "\n";
    if (o==1){goto step17;}
    if (o==2){goto step18;}
step16:
    cout << "就算自己是错的你也不愿意承认吗？\n";
    cout << "1.是的  2.不是\n";
    cin >> p;
    cout << "\n";
    if (p==1){goto step14;}
    if (p==2){goto step17;}
step17:
    cout << "你不会去同化任何人吗？\n";
    cout << "1.是的  2.不是\n";
    cin >> q;
    cout << "\n";
    if (q==1){goto step18;}
    if (q==2){goto step19;}
step18:
    cout << "你希望自己时刻能保持礼貌吗？\n";
    cout << "1.是的  2.不是\n";
    cin >> r;
    cout << "\n";
    if (r==1){goto step19;}
    if (r==2){goto stepBBB;}
step19:
    cout << "你是个讨厌接触自己不熟悉的领域的人吗？\n";
    cout << "1.是的  2.不是\n";
    cin >> s;
    cout << "\n";
    if (s==1){goto stepBBB;}
    if (s==2){goto stepBBB;}
stepAAA:
    cout << "\n\n答案：\n\n";
    cout << "你什么都看不惯，会被社会淘汰的\n";
    cout << "社会进步，表现在各方面，精神文明建设，文化方面都会有大的改观，你是不是有点太保守了，你总是这也看不下去，那也觉得不顺眼\n";
    cout << "你责怪那些稀奇古怪的新新事物，你反感太潮太前卫的东西\n";
    cout << "你应该把眼界放得更开阔一点，否则你不适应社会的发展，你就会被淘汰。 \n";
    cout << "\n";
    goto stepEND;
stepBBB:
    cout << "\n\n答案：\n\n";
    cout << "你什么都懒得管，懒得看必定会被社会淘汰\n";
    cout << "一个人在社会中总是要参与社会的活动的，如果你什么都不管，对什么都闭上眼睛关上耳朵，不见得是一件好事\n";
    cout << "你无法接受到社会传递给你的信息，你也就不知道该怎样去顺应社会\n";
    cout << "长久以来，你必定会被淘汰，你连最近有什么新闻都不知道，怎么跟上社会的脚步呢？\n";
    cout << "\n";
    goto stepEND;
stepCCC:
    cout << "\n\n答案：\n\n";
    cout << "你什么都不想学 当然会被社会淘汰\n";
    cout << "学什么你都觉得辛苦，对于自己不懂的事情又不想去问别人\n";
    cout << "这样子，你永远处于原地踏步的状态，你的生活原地打圈，你的世界是井底之蛙所看到的范围\n";
    cout << "如果你能多充实自己，不要怕辛苦，多去了解自己不了解的领域，你就不会被淘汰了。\n";
    cout << "\n";
    goto stepEND;
stepDDD:
    cout << "\n\n答案：\n\n";
    cout << "你什么都顺从什么都接纳 会导致你被社会淘汰\n";
    cout << "你这样子不行，你都完全没有自己的意见了，别人怎么来欣赏你呢？\n";
    cout << "在别人看来根本就忽视你好了，反正说什么你都没有意见，你都同意，那又何必要说给你听\n";
    cout << "你要学会发表自己的意见，学会思考，不要人云亦云，别人说对的你就不会觉得错。\n";
    cout << "\n";
    goto stepEND;
stepEND:
    cout << "\n谢谢您的耐心，您可以选择 \n";




















}
void ZIBEI(){
int a;int b;int c;
cout << "假如你是一个冒险家\n";
cout << "在一次到埃及的冒险中，你发现了一个地下秘室，潜意识认为，这个秘室会有意想不到的收获\n";
cout << "于是你准备进到秘室，但是，要进去非常难，因为门上布满了机关，而且有美丽的雕刻\n";
cout << "你认为门上哪个图案是能让你进去的机关？\n\n";
cout << "1.美丽的女神\n";
cout << "2.带刺的树枝\n";
cout << "3.古老的法老咒文\n";
cout << "4.大力士像\n";
cin >> a;
if (a==1){
cout << "\n选1：对外表自卑\n";
cout << "你非常不相信自己的外表，认为自己的外表没有一丝漂亮的地方\n";
cout << "而且你不懂得如何装扮，把自己的魅力展现出来\n";
cout << "和别人在一起的时候你只会现在最不引人注意的角落，不让自己觉得不好的一面呈现在人们面前\n";
cout << "其实，每个人都有自己的特色，所以没有必要为自己的外表自卑。\n";
cout << "\n";


}
if (a==2){
cout << "\n选2：对性格自卑\n";
cout << "选择树枝的你对自己的性格非常不满意\n";
cout << "你认为自己的性格充满了弱点，别人一挑就能挑出一大堆毛病\n";
cout << "有时候，你会厌倦自己，而不敢和别人交流，怕别人讨厌你\n";
cout << "其实，你既然知道了自己性格的缺点，就要努力改正。\n";
cout << "\n";


}
if (a==3){
cout << "\n选3：对知识自卑\n";
cout << "法老的咒文代表的是知识和学历\n";
cout << "你觉得自己念的书不多，就应当利用业余时间学习，以充实自己。\n";
cout << "\n";


}
if (a==4){
cout << "\n选4：对自己的运动现象感到自卑\n";
cout << "大力士象征着运动，你选择大力士是因为你对自己的运动形象感到自卑\n";
cout << "其实，对自己的运动表现感到自卑是没必要的\n";
cout << "你觉得自己运动方面不好，就利用空余时间多锻炼\n";
cout << "需要明确的一点是，运动的意义在于一个好的身体\n";
cout << "\n";


}

cout << "1.查看完整答案  2.结束测试\n";
cin >> b;
if (b==2){goto stepEND;}
if (b==1){
cout << "\n选1：对外表自卑\n";
cout << "你非常不相信自己的外表，认为自己的外表没有一丝漂亮的地方\n";
cout << "而且你不懂得如何装扮，把自己的魅力展现出来\n";
cout << "和别人在一起的时候你只会现在最不引人注意的角落，不让自己觉得不好的一面呈现在人们面前\n";
cout << "其实，每个人都有自己的特色，所以没有必要为自己的外表自卑。\n";
cout << "\n";
cout << "选2：对性格自卑\n";
cout << "选择树枝的你对自己的性格非常不满意\n";
cout << "你认为自己的性格充满了弱点，别人一挑就能挑出一大堆毛病\n";
cout << "有时候，你会厌倦自己，而不敢和别人交流，怕别人讨厌你\n";
cout << "其实，你既然知道了自己性格的缺点，就要努力改正。\n";
cout << "\n";
cout << "选3：对知识自卑\n";
cout << "法老的咒文代表的是知识和学历\n";
cout << "你觉得自己念的书不多，就应当利用业余时间学习，以充实自己。\n";
cout << "\n";
cout << "选4：对自己的运动现象感到自卑\n";
cout << "大力士象征着运动，你选择大力士是因为你对自己的运动形象感到自卑\n";
cout << "其实，对自己的运动表现感到自卑是没必要的\n";
cout << "你觉得自己运动方面不好，就利用空余时间多锻炼\n";
cout << "需要明确的一点是，运动的意义在于一个好的身体\n";
cout << "                               \n小贴示：\n";
cout << "很多人都有自卑的时候，因为在某些时候，潜意识会告诉你，你在某些方面不如别人，所以你就感觉比别人差了\n";
cout << "自卑的人，很需要全面的认识自己，善于发现自己的优点，找准自己的真正价值，暗示自己不比别人差\n";
cout << "这样，你才能超越自我\n";
cout << "\n";

goto stepEND;
}
stepEND:
    cout << "\n\n谢谢您的耐心，您可以选择 \n";


}
