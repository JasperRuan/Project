#include <iostream>

using namespace std;
void XINLINGFANGXIAN ();
void RENJIGUANXI();
void STAR();
void EQ();
void SIXUE ();
void HURT ();
void TAOTAI ();
void ZIBEI();
int main()
{
int a;int b;
int c;int d;
int e;int f;
int g;int h;
int i;int j;
int k;int l;
int m;int n;
int o;int p;
int q;int r;
int s;int t;
int u;int v;
int w;int x;
int y;int z;
int AAA;
cout << "欢迎来到Jasper心理测试屋，选择一个你想测试的题\n\n";
step1:
    cout << "1.你的心理防线在哪里\n2.人际关系\n3.你是因为什么追星\n4.情商测试\n5.看看你的人生“死穴”是什么\n";
    cout << "6.你最怕什么样的伤疤被揭开\n7.你会因为什么被社会淘汰\n8.你被什么样的自卑困扰着\n";
    cin >>a;
    cout << "\n";
    if (a==1){XINLINGFANGXIAN();}
    if (a==2){RENJIGUANXI();}
    if (a==3){STAR();}
    if (a==4){EQ();}
    if (a==5){SIXUE ();}
    if (a==6){HURT ();}
    if (a==7){TAOTAI ();}
    if (a==8){ZIBEI ();}
    cout << "\n";
    cout << "1.退出测试    2.再测一题\n";
    cin >> b;
    cout << "\n";
    if (b==1){goto step2;}
    if (b==2){goto step1;}
step2:
    cout << "谢谢您\n";






return 0;
}

void XINLINGFANGXIAN (){
int a;int b;
int c;int d;
int e;int f;
int g;int h;
int i;int j;
int k;int l;
int m;int n;
int o;int p;
int q;int r;
int s;int t;
int u;int v;
int w;int x;
int y;int z;
cout << "                           你的心灵防线在哪里\n\n\n";
cout << "某天，你中了一个挑战旅游奖，来到了一座迷宫面前，你认为迷宫周围的环境会是：\n";
cout << "1.一片香花异草开放，阳光明媚  2.浓浓的黑雾包围着，乌云密布\n";
cin >> a;
cout << "\n";
if (a==1){goto step3;}
if (a==2){goto step2;}
step2:
    cout << "你看到，要到达迷宫必须要经过：\n";
    cout << "1.一个险要的悬崖  2.一条深浅莫测的河流\n";
    cin >> b;
    cout << "\n";
    if (b==1){goto step3;}
    if (b==2){goto step4;}
step3:
    cout << "通向迷宫的唯一工具是：\n";
    cout << "1.一个缺了许多木板的吊桥  2.一个简易拼搭的独木桥\n";
    cin >> c;
    cout << "\n";
    if (c==1){goto step4;}
    if (c==2){goto step5;}
step4:
    cout << "好不容易终于抵达了迷宫门口，隐隐约约看到悬着一块掉了漆的木板，上面写着：\n";
    cout << "1.迷失的人在这迷失，相逢的人在这相逢  2.死并非生的对立面，而作为生的一部分永存\n";
    cin >> d;
    cout << "\n";
    if (c==1){goto step5;}
    if (c==2){goto step6;}
step5:
    cout << "于是，直觉告诉你：\n";
    cout << "1.你终会被困在这里  2.你会在这遇见想见的人\n";
    cin >> e;
    cout << "\n";
    if (e==1){goto step7;}
    if (e==2){goto step6;}
step6:
    cout << "如果允许你带一个同伴，你会选择：\n";
    cout << "1.家人或者朋友  2.一个陌生人\n";
    cin >> f;
    cout << "\n";
    if (f==1){goto step7;}
    if (f==2){goto step8;}
step7:
    cout << "可是你还是按照规则独自探索，一开始，你会选择：\n";
    cout << "1.向右拐  2.向左拐\n";
    cin >> g;
    cout << "\n";
    if (g==1){goto step8;}
    if (g==2){goto step9;}
step8:
    cout << "你看到了一道奇怪的光，你会：\n";
    cout << "1.顺着光的方向走  2.按照自己的既定路线走\n";
    cin >> h;
    cout << "\n";
    if (h==1){goto step9;}
    if (h==2){goto step9;}
step9:
    cout << "在途中，你看到一只脚部受伤的小猫蜷缩在角落里，奄奄一息，这时候你会：\n";
    cout << "1.为它治疗，带着它上路  2.不管它，继续赶路\n";
    cin >> i;
    cout << "\n";
    if (i==1){goto step10;}
    if (i==2){goto step11;}
step10:
    cout << "这只可怜的小猫为你的行程温暖了不少，可你发现它常常在午夜时分不知所踪，你会怎么做：\n";
    cout << "1.装睡，然后偷偷观察  2.可能是猫的习性，并不很在意\n";
    cin >> j;
    cout << "\n";
    if (j==1){goto step11;}
    if (j==2){goto step12;}
step11:
    cout << "走了一大段路，你沮丧的发现自己来到了：\n";
    cout << "1.曾经走过N次的地方  2.有一个死角  3.迷宫的入口\n";
    cin >> k;
    cout << "\n";
    if (k==1){goto step12;}
    if (k==2){goto step13;}
    if (k==3){goto stepD;}
step12:
    cout << "在迷宫里呆了几天，你还是找不到出路，你觉得：\n";
    cout << "1.很害怕，认为自己就快疯了  2.虽然担心，但相信一定能找到出路\n";
    cin >> l;
    cout << "\n";
    if (l==1){goto step13;}
    if (l==2){goto step14;}
step13:
    cout << "在你准备发射信号弹求救的时候，遇到一个神经兮兮的流浪汉，他说能帮你找到出口，但你必须听他唱歌并且由衷赞美他的歌喉，你会怎么做？\n";
    cout << "1.先按他说的做做看  2.看他像是走不出去疯了，还是发信号弹安全\n";
    cin >> m;
    cout << "\n";
    if (m==1){goto step15;}
    if (m==2){goto step14;}
step14:
    cout << "被困在这个地方不能出去，你认为自己欠缺了什么？ \n";
    cout << "1.智力  2.运气  3.意志力\n";
    cin >> n;
    cout << "\n";
    if (n==1){goto stepA;}
    if (n==2){goto stepB;}
    if (n==3){goto stepC;}
step15:
    cout << "如果还有重新游历的经历，你还会来吗？\n";
    cout << "1.还会  2.不会\n";
    cin >> o;
    cout << "\n";
    if (o==1){goto stepE;}
    if (o==2){goto stepC;}

stepA:
    cout << "A类型：\n";
    cout << "是一个心灵无设防的人，平生没有什么大志，也没有什么企图心。\n";
    cout << "在与人交流的时候，你总是全身心地付出，让人一眼就看穿你的真心，虽然给人平凡的印象，却没有烦恼压力。\n";
    cout << "可是这样单纯的你也容易被城府较深的人利用，因此，虽然没有害人之心，但在这个社会里，防人之心却是能够保护你的工具。\n";
    cout << "小贴士：害人之心不可有，防人之心不可无。单纯的你容易被城府较深的人利用，因此，虽然没有害人之心，但在这个社会里，防人之心却是能够保护你的工具。\n";
stepB:
    cout << "B类型：\n";
    cout << "你的心灵几乎不设防，平常做事有点粗枝大叶的你是一个崇尚心灵自由的人，与人交流的时候大部分只顾自己一吐为快。\n";
    cout << "抱着得过且过的生活态度，你的生活虽然看上去无比轻松惬意，但是却藏着许多隐患。因此，你需要多为日后打算，把目光放长远。\n";
    cout << "小贴士：害人之心不可有，防人之心不可无。单纯的你容易被城府较深的人利用，因此，虽然没有害人之心，但在这个社会里，防人之心却是能够保护你的工具。\n";
stepC:
    cout << "C类型：\n";
    cout << "你是一个心灵半设防的人，你有着宽大的心胸，总是助人为乐，愿意帮助朋友摆脱困境，对任何事情都抱着以和为贵的态度。\n";
    cout << "但是，对于自己的某些问题，你总是遮遮掩掩、躲躲藏藏，有封闭内心的倾向，不愿意向人诉说，让人觉得神秘兮兮。\n";
    cout << "不妨多和知心好友聊天，在轻松愉悦的氛围中放开自己的胸怀吧！\n";
    cout << "小贴士：害人之心不可有，防人之心不可无。单纯的你容易被城府较深的人利用，因此，虽然没有害人之心，但在这个社会里，防人之心却是能够保护你的工具。\n";
stepD:
    cout << "D类型：\n";
    cout << "你的心灵防线建筑在不伤害别人的前提之下，对于外界的是非，经过这道防线的过滤，能让你看的比谁都清楚。\n";
    cout << "是一个思路极细的人，对于身边的事物都能有良好的安排，因为有计划，因此大部分事情都在你的掌握之中\n";
    cout << "虽说不上城府极深，但对于复杂的人际关系却能处理的很好，如鱼得水。\n";
    cout << "小贴士：害人之心不可有，防人之心不可无。单纯的你容易被城府较深的人利用，因此，虽然没有害人之心，但在这个社会里，防人之心却是能够保护你的工具。\n";
stepE:
    cout << "E类型：\n";
    cout << "你在自己的心附近筑起了高高的城墙，甚至还挂着“生人勿近”的危险警告牌。因为你对事物的观察异常敏锐，更能看透人心，所以你担心自己的内心轻易被人看穿，总是做一些事或者说一些话来混淆别人的目光。\n";
    cout << "在围墙里呆久了，小心影响心灵的健康哦，虽然不可以在短时间内卸下这道墙，但是何不在墙上开些小洞，让心透透气呢！\n";
    cout << "小贴士：害人之心不可有，防人之心不可无。单纯的你容易被城府较深的人利用，因此，虽然没有害人之心，但在这个社会里，防人之心却是能够保护你的工具。\n";


cout << "\n\n谢谢您的耐心，您可以选择 \n";






}

void RENJIGUANXI(){
cout << "                                人际关系\n\n";
int a;int b;
cout << "假如你走向一个熟睡的婴儿，他忽然睁开眼睛，你认为他接着会有什么反应？\n";
cout << "1.哭\n";
cout << "2.笑\n";
cout << "3.闭上眼睛继续睡觉\n";
cout << "4.咳嗽\n";
cin >> a;
cout << "\n";
if (a==1){
cout << "你是一个相当没有自信的人，因此害怕与大家相处，唯恐泄漏自己的缺点，因此常缩在自己的壳中裹足不前\n";
cout << "如果你能在再自信点，积极与他人接触，相信你会发现外面的世界很美好。\n";
cout << "\n";
cout << "\n";
}
if (a==2){
    cout << "你是一个自信满满的人，交际手腕相当不错，很容易和他人打成一片\n";
    cout << "但要注意，不要过度自信，只陶醉在自己的世界中而忽视其他人的想法、感受。\n";
    cout << "\n";

    }
if (a==3){
    cout << "你是一个相当孤僻的人，与其和别人在一起，还不如一个人来得快乐自由，所以根本不愿，也觉得没有必要踏入别人的世界\n";
    cout << "工作是注重团队合作的，绝不可独来独往，所以要好好调整自己。\n";
    cout << "\n";
    cout << "\n";}
if (a==4){
    cout << "你是一个相当神经质的人，非常在乎人际关系，也小心翼翼的去维护，\n";
    cout << "但太过在意别人的感情、想法会弄的自己精疲力尽，最好放松一下自己，以平常心态来面对人际关系。\n";
    cout << "\n";
    cout << "\n";}
cout << "请选择：\n";
cout << "1.查看完整答案  2.结束本测试\n";
cin >> b;
if (b==1){goto step2;}
if (b==2){goto step1;}
cout << "\n\n";
step2:
    cout << "\n选A的人：\n你是一个相当没有自信的人，因此害怕与大家相处，唯恐泄漏自己的缺点，因此常缩在自己的壳中裹足不前\n";
cout << "如果你能在再自信点，积极与他人接触，相信你会发现外面的世界很美好。\n\n";
cout << "选B的人：\n你是一个自信满满的人，交际手腕相当不错，很容易和他人打成一片\n";
cout << "但要注意，不要过度自信，只陶醉在自己的世界中而忽视其他人的想法、感受。\n\n";
cout << "选C的人：\n你是一个相当孤僻的人，与其和别人在一起，还不如一个人来得快乐自由，所以根本不愿，也觉得没有必要踏入别人的世界\n";
cout << "工作是注重团队合作的，绝不可独来独往，所以要好好调整自己。\n\n";
cout << "选D的人：\n你是一个相当神经质的人，非常在乎人际关系，也小心翼翼的去维护，但太过在意别人的感情、想法会弄的自己精疲力尽\n";
cout << "最好放松一下自己，以平常心态来面对人际关系。\n\n\n";
cout << "                                小贴士\n婴儿代表他人，一般人对于他人相处容易产生不安与恐惧\n";
cout << "越是没有自信的人就越严重，出于这种不安，不信任易于言表，于是对方产生同样反应\n";

step1:
    cout << "\n\n谢谢您的耐心，您可以选择\n";


}

void STAR(){
    int a;
    int b;
    int c;
    int d;
    int e;
    int f;
    int g;
    int h;
    int i;
    int j;
    int k;
    int l;
    int m;
    int n;
    int o;
    int p;
    int q;
    int r;
    int s;
    int t;
    int u;
    int v;
    int w;
    int x;
    int y;
    int z;
    int A;
    int B;
    int C;
    int D;
    int E;
    int F;
    int G;
    int H;
    int I;
    int J;
    int K;
    int L;
    int M;
    int N;
    int O;
    int P;
    int Q;
    int R;
    int S;
    int T;
    int U;
    int V;
    int W;
    int X;
    int Y;
    int Z;
    int AA;
    cout << "我们现在来做一个小测试，测试你追星的理由 \n";
    cout << "\n";
    cout << "请您回答以下的一些列问题，输入选项并按下回车键 \n";
    cout << "\n";
    cout << "无论偶像出什么样的周边，你都会买吗？\n";
    cout << "1.是的   2.不是 \n";
    cout << "答案 ";
    cin >> a;
    if (a==1){
        cout << "\n";
        cout << "你认识很多其他的粉丝吗？ \n";
        cout << "1.是的   2.不是 \n";
        cout << "答案 ";
        cin >> b;
        if (b==1){
            cout << "\n";
            cout << "你和其他的粉丝会聊除了偶像之外的话题吗？ \n";
            cout << "1.会   2.不会 \n";
            cout << "答案 ";
            cin >> c;
            if (c==1){
                cout << "\n";
                cout << "你愿意掏出自己攒下的积蓄去看一场偶像的演唱会吗？ \n";
                cout << "1.愿意   2.不愿意 \n";
                cout << "答案 ";
                cin >> d;
                if (d==1){
                    cout << "\n";
                    cout << "你的父母知道你追星吗？ \n";
                    cout << "1.知道   2.不知道 \n";
                    cout << "答案 ";
                    cin >> e;
                    if (e==1){
                        cout << "\n";
                        cout << "你的偶像和你的理想型相符吗？ \n";
                        cout << "1.相符   2.不相符 \n";
                        cout << "答案 ";
                        cin >> f;
                        if (f==1){
                            cout << "\n";
                            cout << "你称呼你的偶像不用他的全名吗？ \n";
                            cout << "1.是的   2.不是 \n";
                            cout << "答案 ";
                            cin >> g;
                            if (g==1){
                                cout << "\n";
                                cout << "\n";
                                cout << "A.追星是因为寂寞，而偶像能满足你想要的一切。 \n";
                            }
                            if (g==2){
                                cout << "\n";
                                cout << "\n";
                                cout << "E.追星是因为你将偶像是为生活中的一部分。";
                            }
                        }
                        if (f==2){
                            cout << "\n";
                            cout << "你觉得你和你的偶像有共同点吗？ \n";
                            cout << "1.有    2.没有 \n";
                            cout << "答案 ";
                            cin >> h;
                            if (h==1){
                                cout << "\n";
                                cout << "\n";
                                cout << "B.追星是因为你能从他身上获得动力和勇气。 \n";
                            }
                            if (h==2){
                                cout << "\n";
                                cout << "你心情不好的时候会想到你的偶像吗？ \n";
                                cout << "1.会    2.不会 \n";
                                cout << "答案 ";
                                cin >> i;
                                if (i==1){
                                    cout << "\n";
                                    cout << "\n";
                                    cout << "D.追星让你觉得充满了安全感和归属感。 \n";
                                }
                                if (i==2){
                                    cout << "\n";
                                    cout << "\n";
                                    cout << "C.追星让你感觉到快乐。 \n";
                                }
                            }

                        }
                    }
                    if (e==2){
                        cout << "\n";
                        cout << "你基本不会因为追星影响生活？ \n";
                        cout << "1.是的   2.不是 \n";
                        cout << "答案 ";
                        cin >> j;
                        if (j==1){
                            cout << "\n";
                            cout << "你会偶像哭过吗？ \n";
                            cout << "1.哭过   2.没有哭过 \n";
                            cout << "答案 ";
                            cin >> k;
                            if (k==1){
                                cout << "\n";
                                cout << "你的偶像会给你带来前进的动力吗？ \n";
                                cout << "1.会   2.不会 \n";
                                cout << "答案 ";
                                cin >> l;
                                if (l==1){
                                    cout << "\n";
                                    cout << "你觉得你和你的偶像有共同点吗？ \n";
                                    cout << "1.有    2.没有 \n";
                                    cout << "答案 ";
                                    cin >> m;
                                    if (m==1){
                                        cout << "\n";
                                        cout << "\n";
                                        cout << "B.追星是因为你能从他身上获得动力和勇气。 \n";
                                    }
                                    if (m==2){
                                        cout << "\n";
                                        cout << "你心情不好的时候会想到你的偶像吗？ \n";
                                        cout << "1.会    2.不会 \n";
                                        cout << "答案 ";
                                        cin >> n;
                                        if (n==1){
                                            cout << "\n";
                                            cout << "\n";
                                            cout << "D.追星让你觉得充满了安全感和归属感。 \n";
                                        }
                                        if (n==2){
                                            cout << "\n";
                                            cout << "\n";
                                            cout << "C.追星让你感觉到快乐 \n";
                                        }
                                    }
                                }
                                if (l==2){
                                    cout << "\n";
                                    cout << "你心情不好的时候会想到你的偶像吗？ \n";
                                    cout << "1.会    2.不会 \n";
                                    cout << "答案 ";
                                    cin >> o;
                                    if (o==1){
                                        cout << "\n";
                                        cout << "\n";
                                        cout << "D.追星让你觉得充满了安全感和归属感 \n";
                                    }
                                    if (o==2){
                                        cout << "\n";
                                        cout << "\n";
                                        cout << "C.追星让你感觉到快乐 \n";
                                    }
                                }
                            }
                            if (k==2){
